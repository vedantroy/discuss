{
  "incremental": true,
  "lineWidth": 85,
  "typescript": {
  },
  "json": {
  },
  "markdown": {
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md}"],
  "excludes": [
    "**/public",
    "**/node_modules",
    "**/*-lock.json",
    "**/build",
    "**/.data"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.64.2.wasm",
    "https://plugins.dprint.dev/json-0.14.1.wasm",
    "https://plugins.dprint.dev/markdown-0.12.2.wasm"
  ]
}
